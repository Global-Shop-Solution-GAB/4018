program.sub.btnprint_click.start
V.Local.sError.Declare
V.Local.sRet.Declare
V.Local.iBIRunID.Declare
V.Local.iLogID.Declare
V.Local.iCount.Declare
V.Local.sParam.Declare
V.Local.sVal.Declare
F.Intrinsic.Control.SetErrorHandler("Print_Err")
F.Intrinsic.Control.ClearErrors

F.Intrinsic.UI.Msgbox("Are you sure you want to export the information in the grid to your report?","Export BINs to Report",1,V.Local.sRet)
F.Intrinsic.Control.If(V.Local.sRet,=,1)
	F.Global.BI.GetRunID(V.Local.iBIRunID)
	F.Global.BI.StartLogging(V.Local.iBIRunID,-1,-1,"",V.Local.iLogID)
	F.Intrinsic.UI.InvokeWaitDialog("Generating the records for your report. This may take a few minutes.")

	F.Intrinsic.Control.For(V.Local.iCount,0,V.uGlobal.uBINS.UBound,1)
		V.uGlobal.uBINS(V.Local.iCount)!BIRUNID.Set(V.Local.iBIRunID)
'		V.uGlobal.uBINS(V.Local.iCount)!CYCLE_DATE_START.Set(V.Global.sStartDate)
		V.uGlobal.uBINS(V.Local.iCount)!CYCLE_DATE_END.Set(V.Global.sENDDate)
	F.Intrinsic.Control.Next(V.Local.iCount)
	F.Intrinsic.Variable.UDTMultiQuickSort(V.uGlobal.uBINS!CYCLE_DATE_START,False)
	'F.Intrinsic.Variable.UDTMultiSeek(V.uGlobal.uCurrP!Part,V.uGlobal.uInv(V.Local.i)!Part,V.uGlobal.uCurrP!SC,V.uGlobal.uInv(V.Local.i)!SC)
	F.Intrinsic.Variable.UDTMultiSeek(V.uGlobal.uBINS!PART,"",V.Local.sRet)
	'Function.Intrinsic.Variable.UDTFlagOrdinal(UDT name, Flag/Unflag as boolean,ordinal positions as string)
	F.Intrinsic.Variable.UDTFlagOrdinal(V.uGlobal.uBINS,True,V.Local.sRet)
	F.Intrinsic.Variable.UDTDeleteFlagged(V.uGlobal.uBINS)

	F.Intrinsic.String.Concat("DELETE from GCG_4018_BIN","",V.Global.sQuery)
	F.ODBC.Connection!conx.Execute(V.Global.sQuery)

	F.Intrinsic.String.Concat("SELECT * FROM GCG_4018_BIN","",V.Global.sQuery)
	F.ODBC.Connection!Conx.OpenRecordsetRW("rstSaveToTable",V.Global.sQuery)
	F.Intrinsic.Variable.SaveUDTToRecordSet("Conx","rstSaveToTable","V.uGlobal.uBINS")

	F.ODBC.Conx!rstSaveToTable.Close
	F.Intrinsic.UI.CloseWaitDialog
	F.Global.BI.StopLogging(V.Local.iLogID)
	V.Local.sParam.Set("BIRUNID")
	V.Local.sVal.Set(V.Local.iBIRunID)
	'Print the Report
	F.Global.BI.RunReportPreProcessor(V.Local.iBIRunID,V.Local.iLogID,V.Local.sParam,V.Local.sVal,"",-1,True,"",-1,"",-1,"","",V.Local.iBIRunID)


	F.Intrinsic.Control.CallSub(Clearform)
F.Intrinsic.Control.Else
	F.Intrinsic.Control.ExitSub
F.Intrinsic.Control.EndIF

