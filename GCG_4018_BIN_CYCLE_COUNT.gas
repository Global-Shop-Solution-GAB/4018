Program.Sub.FillFlexgrid.Start
V.Local.iOPtion.Declare(Long,3)
V.Local.sWhereStatement.Declare
V.Local.sStartDate.Declare
V.Local.sSDate.Declare
V.Local.sEndDate.Declare
V.Local.sEDate.Declare
V.Local.iUDTRowCount.Declare(Long,-1)
V.Local.sError.Declare
V.Local.sret.Declare
F.Intrinsic.Control.SetErrorHandler("FillFlexGrid_Err")
F.Intrinsic.Control.ClearErrors

V.Local.sStartDate.Set(V.Screen.F_CcountBIN!dtpStart.Value)
V.Local.sEndDate.Set(V.Screen.F_CcountBIN!dptEndDate.Value)

F.Intrinsic.String.Split(V.Local.sStartDate,"/",V.Local.sStartDate)
F.Intrinsic.String.LPad(V.Local.sStartDate(0),"0",2,V.Local.sStartDate(0))
F.Intrinsic.String.LPad(V.Local.sStartDate(1),"0",2,V.Local.sStartDate(1))
F.Intrinsic.String.Right(V.Local.sStartDate(2),2,V.Local.sStartDate(2))
F.Intrinsic.String.Concat(V.Local.sStartDate(2),V.Local.sStartDate(0),V.Local.sStartDate(1),V.Local.sSDate)
V.Global.sStartDate.Set(V.Local.sSDate)

F.Intrinsic.String.Split(V.Local.sEndDate,"/",V.Local.sEndDate)
F.Intrinsic.String.LPad(V.Local.sEndDate(0),"0",2,V.Local.sEndDate(0))
F.Intrinsic.String.LPad(V.Local.sEndDate(1),"0",2,V.Local.sEndDate(1))
F.Intrinsic.String.Right(V.Local.sEndDate(2),2,V.Local.sEndDate(2))
F.Intrinsic.String.Concat(V.Local.sEndDate(2),V.Local.sEndDate(0),V.Local.sEndDate(1),V.Local.sEDate)
V.Global.sEndDate.Set(V.Local.sEDate)

F.Intrinsic.Control.If(V.Global.bAllDates,=,True)
	F.Intrinsic.Math.Sub(V.Local.iOPtion,2,V.Local.iOPtion)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.If(V.Global.bAllBINS,=,True)
	F.Intrinsic.Math.Sub(V.Local.iOPtion,1,V.Local.iOPtion)
F.Intrinsic.Control.EndIf

F.Intrinsic.Control.SelectCase(V.Local.iOPtion)
	F.Intrinsic.Control.Case(1)
		'All dates with BIN range
		F.Intrinsic.String.Concat(" AND ITEM_MASTER.BIN BETWEEN '",V.Global.sStartBIN,"' AND '",V.Global.sEndBIN,"'",V.local.sWhereStatement)
	F.Intrinsic.Control.Case(2)
		'Date Range, All BINS
		F.Intrinsic.String.Concat(" AND INVENTORY_MST2.DATE_CYCLE BETWEEN '",V.Local.sSDate,"' AND '",V.Local.sEDate,"'",V.local.sWhereStatement)

	F.Intrinsic.Control.Case(3)
		'Date Range and BIN Range
		F.Intrinsic.String.Concat(" AND INVENTORY_MST2.DATE_CYCLE BETWEEN '",V.Local.sSDate,"' AND '",V.Local.sEDate,"' AND ITEM_MASTER.BIN BETWEEN'",V.Global.sStartBIN,"' AND '",V.Global.sEndBIN,"'",V.Local.sWhereStatement)
F.Intrinsic.Control.EndSelect

F.Intrinsic.UI.InvokeWaitDialog("Getting records from the database. This may take a few minutes.")

F.Intrinsic.String.Concat("SELECT DISTINCT INVENTORY_MSTR.PART, INVENTORY_MSTR.CODE_ABC, INVENTORY_MSTR.DESCRIPTION, INVENTORY_MSTR.UM_INVENTORY,SUM(ITEM_MASTER.QUANTITY) AS QTY_ONHAND, INVENTORY_MST2.DATE_CYCLE,ITEM_MASTER.BIN, ","",V.Global.sQuery)
F.Intrinsic.String.Concat(V.Global.sQuery,"BIN_MASTER.BIN_DESCRIPTION, ' ' AS BIRUNID, INVENTORY_MST2.DATE_CYCLE AS CYCLE_DATE_START, ' ' AS CYCLE_DATE_END FROM INVENTORY_MSTR LEFT OUTER JOIN INVENTORY_MST2 ON INVENTORY_MSTR.PART = INVENTORY_MST2.PART ","",V.Global.sQuery)
F.Intrinsic.String.Concat(V.Global.sQuery,"LEFT OUTER JOIN ITEM_MASTER ON INVENTORY_MSTR.PART = ITEM_MASTER.PART LEFT OUTER JOIN BIN_MASTER ON ITEM_MASTER.BIN = BIN_MASTER.BIN WHERE ITEM_MASTER.BIN <> '' ",V.Local.sWhereStatement," GROUP BY INVENTORY_MSTR.PART, ","",V.Global.sQuery)
F.Intrinsic.String.Concat(V.Global.sQuery,"INVENTORY_MSTR.CODE_ABC, INVENTORY_MSTR.DESCRIPTION, INVENTORY_MSTR.UM_INVENTORY,ITEM_MASTER.BIN,BIN_MASTER.BIN_DESCRIPTION, INVENTORY_MST2.DATE_CYCLE ORDER BY INVENTORY_MST2.DATE_CYCLE, INVENTORY_MSTR.PART","",V.Global.sQuery)



F.ODBC.Connection!Conx.OpenRecordsetRO("rstBINS",V.Global.sQuery)

F.Intrinsic.Variable.LoadUDTFromRecordset("Conx","rstBINS","uGlobal.uBINS","FALSE",100000)

F.ODBC.Conx!rstBINS.Close

V.Global.iRowCount.Set(V.uGlobal.uBINS.UBound)
F.Intrinsic.UI.CloseWaitDialog
F.Intrinsic.Control.If(V.uGlobal.uBINS.UBound,>,-1)
	Gui.F_CcountBIN.GSFGCycleCount.BuildStyle(1,0,"Format","Alignment",2)
	Gui.F_CcountBIN.GSFGCycleCount.BuildStyle(1,1,"Format","Alignment",2)
	Gui.F_CcountBIN.GSFGCycleCount.BuildStyle(1,2,"Format","Alignment",2)
	Gui.F_CcountBIN.GSFGCycleCount.BuildStyle(1,3,"Format","Alignment",2)
	Gui.F_CcountBIN.GSFGCycleCount.BuildStyle(1,4,"Format","Alignment",4)
	Gui.F_CcountBIN.GSFGCycleCount.BuildStyle(1,4,"Type","Command","Delete")
	Gui.F_CcountBIN.GSFGCycleCount.Visible(FALSE)
	F.Intrinsic.UI.InvokeWaitDialog("Loading screen with the selected BINS. This may take several minutes.")
	F.Intrinsic.Variable.UDTToString(V.uGlobal.uBINS,"PART*!*BIN*!*BIN_DESCRIPTION*!*DATE_CYCLE*!*BIN","!*!","*!*",v.Local.sRet)
	Gui.F_CcountBIN.GSFGCycleCount.LoadFromString(V.Local.sret,1)

	'Gui.F_CcountBIN.GSFGCycleCount.LoadFromUDT("	V.uGlobal.uBINS","PART*!*BIN*!*BIN_DESCRIPTION*!*DATE_CYCLE*!*BIRUNID",1)
	V.Global.iFGStyle.Set(1)
	F.Intrinsic.Control.CallSub(Flexgridformatting)
	F.Intrinsic.UI.CloseWaitDialog
	Gui.F_CcountBIN.GSFGCycleCount.Visible(TRUE)
F.Intrinsic.Control.Else
	F.Intrinsic.UI.Msgbox("Your search criteria did not return any records for the date(s) selected.")
	F.Intrinsic.Control.CallSub(Clearform)
F.Intrinsic.Control.EndIf

